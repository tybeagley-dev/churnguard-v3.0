# ChurnGuard 3.0 Environment Variables

# Development mode (set to 'demo' to use mock data, 'production' for real BigQuery)
NODE_ENV=development
DEMO_MODE=true

# Google Cloud BigQuery Configuration (fill in your actual values)
GOOGLE_CLOUD_PROJECT_ID=
GOOGLE_CLOUD_PRIVATE_KEY_ID=
GOOGLE_CLOUD_PRIVATE_KEY=
GOOGLE_CLOUD_CLIENT_EMAIL=
GOOGLE_CLOUD_CLIENT_ID=
GOOGLE_CLOUD_CLIENT_X509_CERT_URL=

# BigQuery Dataset and Tables
BIGQUERY_DATASET=churnguard_data
BIGQUERY_ACCOUNTS_TABLE=account_metrics
BIGQUERY_HISTORICAL_TABLE=historical_performance
BIGQUERY_TRENDS_TABLE=monthly_trends

# Server Configuration
PORT=8000